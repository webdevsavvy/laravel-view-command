{
    "name": "webdevsavvy/laravel-view-command",
    "description": "A simple console command for generating blade template views",
    "keywords": ["webdevsavvy", "laravel-view-command"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Abreu Cabrera",
            "email": "devmail.adrianabreu@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Webdevsavvy\\LaravelViewCommand\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webdevsavvy\\LaravelViewCommand\\LaravelViewCommandServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
